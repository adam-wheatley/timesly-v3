{
  "name": "timesly-api",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn generate && tsc",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "migrate": "prisma2 migrate save --experimental",
    "migrate-up": "prisma2 migrate up --experimental",
    "postinstall": "yarn generate",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/index"
  },
  "dependencies": {
    "@prisma/client": "2.0.0-preview025",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node-fetch": "2.5.5",
    "apollo-server-express": "^2.11.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "14.6.0",
    "jsonwebtoken": "^8.5.1",
    "nexus": "0.12.0-rc.14",
    "nexus-prisma": "0.11.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0-preview025",
    "@types/node": "12.12.32",
    "ts-node": "8.8.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
